{
	"name": "vscode-terminal-git-palette",
	"displayName": "Git Command Palette",
	"description": "Eine praktische Git-Befehls-Palette f√ºr das VS Code Terminal.",
	"version": "0.0.1",
	"publisher": "Ebixx",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ebiixx/vsc-git-terminal-command-palette"
	},
	"engines": {
		"vscode": "^1.80.0"
	},
	"enabledApiProposals": [
		"terminalDataWriteEvent",
		"terminalDimensions"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "GitTerminal.gitCommandPalette",
				"title": "Git Command Palette"
			}
		],
		"terminal": {
			"profiles": [
				{
					"id": "GitTerminal.terminal-profile",
					"icon": "terminal",
					"title": "Terminal API Profile"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint",
		"download-api": "dts dev",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.80.0",
		"eslint": "^9.13.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.26.0"
	},
	"dependencies": {
		"@vscode/dts": "^0.4.1"
	}
}
